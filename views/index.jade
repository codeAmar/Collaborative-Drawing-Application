extends layout

block content
  .master
    #contentwrap
      .username
        p Enter your username: 
        form#usernameform
          input#username(placeholder='Username')
          br
          button#submit Submit
      #chatwrap
        .overflow
          ul#messages
        form#chatting(action='')
          input#messagebox
          div
            img#warning(src='data:image/svg+xml;utf8;base64,')
            p.typewarning Please type a message first!
          button#send Send
          button#clear Clear Chat
    #logo
      img(src="/images/project.png")
    #partition2
      h3 Connected Users:
      br
      p (Click on username to send the drawing request)
      #allusers
  
